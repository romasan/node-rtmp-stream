{
  "name": "rtmp-stream",
  "version": "1.1.0",
  "description": "",
  "scripts": {
    "start": "parcel pages/index.html pages/*/index.html",
    "prebuild": "rm -rf dist/*.*",
    "build": "parcel build pages/index.html pages/*/index.html --no-source-maps",
    "serve": "cd ./server && node index.js",
    "tools": "node ./server/tools/index.js",
    "recover": "npm run tools recover ./server/pixels.log ./server/426x240.png ./server/inout.png",
    "clearcanvas": "npm run tools drawDefaultCanvas ./server/inout.png 240 240 WHITE",
    "render": "npm run tools drawSteps ./server/pixels.log ./server/426x240.png"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "canvas": "^2.11.0",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.3",
    "emittery": "^1.0.1",
    "is-mobile": "^4.0.0",
    "node-fetch": "^2.6.9",
    "node-sass": "^8.0.0",
    "parcel": "^2.8.3",
    "parcel-bundler": "^1.12.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "res": "^0.4.0",
    "src": "^1.1.2",
    "typescript": "^5.0.2",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "postcss": "^8.4.21",
    "postcss-assets": "^6.0.0",
    "postcss-modules": "^4.3.1",
    "typescript-plugin-css-modules": "^5.0.0",
    "yargs": "^17.7.2"
  }
}
