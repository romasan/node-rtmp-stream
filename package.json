{
  "name": "rtmp-stream",
  "version": "1.1.0",
  "description": "",
  "scripts": {
    "start": "parcel pages/index.html pages/*/index.html",
    "prebuild": "rm -rf dist/*.* .parcel-cache",
    "build": "parcel build pages/*.html pages/*/index.html --no-source-maps",
    "postbuild": "cp static/* dist/",
    "render": "react-snap && node ./pages/postbuild.js",
    "serve": "cd ./server && node index.js",
    "tools": "node ./server/tools/index.js",
    "eslint": "./node_modules/.bin/eslint 'src/**/*.{ts,tsx}' --cache"
  },
  "reactSnap": {
    "source": "dist",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "author": "Roman Bauer",
  "license": "ISC",
  "dependencies": {
    "canvas": "^2.11.0",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.3",
    "event-emitter": "^0.3.5",
    "is-mobile": "^4.0.0",
    "node-fetch": "^2.6.9",
    "node-sass": "^8.0.0",
    "parcel": "^2.10.3",
    "parcel-bundler": "^1.12.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "res": "^0.4.0",
    "src": "^1.1.2",
    "typescript": "^5.0.2",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.10.2",
    "@parcel/transformer-sass": "^2.10.3",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "form-data": "^4.0.0",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "postcss": "^8.4.21",
    "postcss-assets": "^6.0.0",
    "postcss-modules": "^4.3.1",
    "react-snap": "^1.23.0",
    "typescript-plugin-css-modules": "^5.0.0",
    "yargs": "^17.7.2"
  }
}
