{
    "compilerOptions": {
        "target": "es2016",
        "lib": [
            "es6",
            "dom"
        ],
        "module": "commonjs",
        "rootDir": ".",
        "resolveJsonModule": true,
        "outDir": "../lib",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}